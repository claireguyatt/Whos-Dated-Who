function dhDisplayToggle(show,hide) {
	document.getElementById('ff-dating-history-'+show).style.display = 'block';
	document.getElementById('ff-dh-btn-'+show).className += ' ff-active';

	var h = hide.split(',');

	document.cookie = "dating_history_show=" + show;
	document.cookie = "dating_history_hide=" + hide;

	for(i = 0; i < h.length; i++) {
    	document.getElementById('ff-dating-history-'+h[i]).style.display = 'none';
