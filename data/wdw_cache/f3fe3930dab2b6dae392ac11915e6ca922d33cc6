        if (c.indexOf('dating_history_show=') == 0) {
            dhs = c.substring(20, c.length);
        }
        if (c.indexOf('dating_history_hide=') == 0) {
            dhh = c.substring(20, c.length);
        }
    }
    
    if(dhs && dhh) {
        dhDisplayToggle(dhs,dhh);
    }
    
}
//--></script><div class="ff-dating-history ff-panel"><div class="ff-dating-history-controls"><button id="ff-dh-btn-grid" class="tiny radius secondary ff-active" onClick="dhDisplayToggle('grid','list,table')"><i class="icon-th"></i> Grid
